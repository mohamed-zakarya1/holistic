<Application x:Class="holistic.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:holistic"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <Style x:Key="forLabels" TargetType="Label">
            <Setter Property="FontSize" Value="30"></Setter>
            <Setter Property="Background" Value="white"></Setter>
            <Setter Property="Margin" Value="42,9,574,394"></Setter>
            <Setter Property="BorderBrush" Value="Blue"></Setter>
            <Setter Property="Foreground" Value="Black"></Setter>
            <Setter Property="FontFamily" Value="TimesNewRoman"></Setter>
        </Style>
        <Style x:Key="forButtons" TargetType="Button">
            <Setter Property="FontSize" Value="30"></Setter>
            <Setter Property="Background" Value="white"></Setter>
            <Setter Property="Margin" Value="42,9,574,394"></Setter>
            <Setter Property="BorderBrush" Value="Blue"></Setter>
            <Setter Property="Foreground" Value="Black"></Setter>
            <Setter Property="FontFamily" Value="TimesNewRoman"></Setter>
        </Style>
    </Application.Resources>
</Application>
